@model ExamManager.Domain.DomainModel.Student

@{
    ViewData["Title"] = "Детали";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Детали</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label>Индекс</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BrojNaIndeks)
        </dd>
        <dt class = "col-sm-2">
            <label>Име и презиме</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ImePrezime)
        </dd>
        <dt class = "col-sm-2">
            <label>Спроведувач</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StudentPolagaVoTermin.SproveduvacId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.BrojNaIndeks">Измени</a> |
    <a asp-action="Index">Назад кон листа на студенти</a>
</div>