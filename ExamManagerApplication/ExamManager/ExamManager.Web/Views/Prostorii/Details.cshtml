@model ExamManager.Domain.DomainModel.Prostorija

@{
    ViewData["Title"] = "Детали";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Детали</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label>Просторија</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ime)
        </dd>
        <dt class = "col-sm-2">
            <label>Семестар</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Kapacitet)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Измени</a> |
    <a asp-action="Index">Назад кон листа на простории</a>
</div>